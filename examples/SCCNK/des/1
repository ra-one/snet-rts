rm *.o boxes/*.a boxes/*.o .libs/* simpleDes

cd ./boxes

gcc  -I/shared/nil/snetInstall/include/snet/ -c desBoxes.c

ar rcs libboxes.a desBoxes.o

cd ..


gcc -std=gnu99 -DPACKAGE_NAME=SNet -DPACKAGE_TARNAME=snet -DPACKAGE_VERSION=1.x "-DPACKAGE_STRING=SNet 1.x" -DPACKAGE_BUGREPORT=info@snet-home.org -DPACKAGE_URL= -DSTDC_HEADERS=1 -DHAVE_SYS_TYPES_H=1 -DHAVE_SYS_STAT_H=1 -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_MEMORY_H=1 -DHAVE_STRINGS_H=1 -DHAVE_INTTYPES_H=1 -DHAVE_STDINT_H=1 -DHAVE_UNISTD_H=1 -D__EXTENSIONS__=1 -D_ALL_SOURCE=1 -D_GNU_SOURCE=1 -D_POSIX_PTHREAD_SEMANTICS=1 -D_TANDEM_SOURCE=1 -DHAVE_PTHREAD=1 -DHAVE_PTHREAD_SETAFFINITY_NP=1 -DHAVE_PTHREAD_YIELD=1 -DHAVE_SYNC_ATOMIC_BUILTINS=1 -DTLSSPEC=__thread -DHAVE___THREAD=1 -DHAVE_DLFCN_H=1 -DLT_OBJDIR=.libs/ -I./include -I/shared/nil/snetInstall/include/snet/ -g -O0 -Wall -pthread -c simpleDes.c -I/shared/nil/snetInstall/include/snet/ -DSNetMain__simpleDes=main  -fPIC -DPIC -o .libs/simpleDes.o

gcc -std=gnu99 -DPACKAGE_NAME=SNet -DPACKAGE_TARNAME=snet -DPACKAGE_VERSION=1.x "-DPACKAGE_STRING=SNet 1.x" -DPACKAGE_BUGREPORT=info@snet-home.org -DPACKAGE_URL= -DSTDC_HEADERS=1 -DHAVE_SYS_TYPES_H=1 -DHAVE_SYS_STAT_H=1 -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_MEMORY_H=1 -DHAVE_STRINGS_H=1 -DHAVE_INTTYPES_H=1 -DHAVE_STDINT_H=1 -DHAVE_UNISTD_H=1 -D__EXTENSIONS__=1 -D_ALL_SOURCE=1 -D_GNU_SOURCE=1 -D_POSIX_PTHREAD_SEMANTICS=1 -D_TANDEM_SOURCE=1 -DHAVE_PTHREAD=1 -DHAVE_PTHREAD_SETAFFINITY_NP=1 -DHAVE_PTHREAD_YIELD=1 -DHAVE_SYNC_ATOMIC_BUILTINS=1 -DTLSSPEC=__thread -DHAVE___THREAD=1 -DHAVE_DLFCN_H=1 -DLT_OBJDIR=.libs/ -I./include -I/shared/nil/snetInstall/include/snet/ -g -O0 -Wall -pthread -c simpleDes.c -I/shared/nil/snetInstall/include/snet/ -DSNetMain__simpleDes=main -o simpleDes.o >/dev/null 2>&1


gcc -std=gnu99 -g -O0 -Wall -pthread .libs/simpleDes.o -o simpleDes -static -Wl,-lruntimestream -Wl,-ltblpel_hrc  -L/shared/nil/snetInstall/lib/snet/ /shared/nil/snetInstall/lib/snet/libC4SNet.a -L. -Lboxes /shared/nil/snetInstall/lib/snet/libsnetutil.a -lboxes /shared/nil/snetInstall/lib/snet/libruntimestream.a /shared/nil/snetInstall/lib/snet/libdistribnodist.a /shared/nil/snetInstall/lib/snet/libtblpel_hrc.a /shared/nil/snetInstall/lib/liblpel_hrc.a /shared/nil/snetInstall/lib/libpcl.a -lpthread -lrt -lm -pthread
