  <metadata>
   <boxdefault>
     <interface value="C4SNet" />
   </boxdefault>
  </metadata>

net simpleDes {

  box snet_source((<interval>, <mess>) -> (pt,key));
  box initP((pt, key) -> (D, K, <C>));
  box subRound((D, K, <C>) -> (D, K, <C>));
  box finalP( (D, K, <C>) -> (ct));
  box snet_sink((ct) -> );


} connect snet_source ..
  initP .. subRound .. subRound .. finalP
  .. snet_sink;



